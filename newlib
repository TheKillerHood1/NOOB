local tweenspeed = 0.2

firstpage = false
lastopened = nil
local library = {}

function library:CreateWindow(name,toggle)

	name = name or "TEST"

	for i,v in pairs(game.CoreGui:GetChildren()) do
		if v.Name == "Killer-Lab" then
			v:Remove()
			if _G.work  then
				_G.work:Close()
				print("CLOSED")
				_G.work = false
			end
		end
	end

	--create holder
	local ScreenGui = Instance.new("ScreenGui")
	
	--CREATE MOVING FRAME
