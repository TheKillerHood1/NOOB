	Holder.Position = UDim2.new(0.353, 0,0.184, 0) --0.324702591, 0, 0.183890581, 0
	Holder.Size = UDim2.new(0, 1,0, 1) --MAIN SIZE: 0, 444, 0, 416

	_4pxShadow2px.Name = "4pxShadow(2px)"
