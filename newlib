	UICorner.Parent = Holder

	local e = TS:Create(Holder, TweenInfo.new(0.3), {Size = UDim2.new(0, 444,0, 35)})
	e:Play()
	e.Completed:Wait()
	local x = TS:Create(Holder, TweenInfo.new(0.3), {Size = UDim2.new(0, 444, 0, 416)})
	x:Play()
	x.Completed:Wait()


	--END

	function dragify(Frame)
		dragToggle = nil
		local dragSpeed = 0.25
		dragInput = nil
		dragStart = nil
		local dragPos = nil
		function updateInput(input)
			local Delta = input.Position - dragStart
			local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
			game:GetService("TweenService"):Create(Holder, TweenInfo.new(0.25), {Position = Position}):Play()
		end
		Frame.InputBegan:Connect(function(input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
				dragToggle = true
				dragStart = input.Position
				startPos = Frame.Position
				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragToggle = false
					end
				end)
			end
		end)
		Frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)
		game:GetService("UserInputService").InputChanged:Connect(function(input)
			if input == dragInput and dragToggle then
				updateInput(input)
			end
		end)
	end


	dragify(Holder)

	function mouseonbutton(frame)
		frame.MouseEnter:Connect(function()
			--Container.BackgroundTransparency = 0.95	
			--TS:Create(frame, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(89, 88, 90)}):Play()
			TS:Create(frame, TweenInfo.new(0.2), {BackgroundTransparency = 0.3}):Play()
		end)

		frame.MouseLeave:Connect(function()
			--Container.BackgroundTransparency = 1
			--TS:Create(frame, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(99, 98, 100)}):Play()
			TS:Create(frame, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
		end)
	end
	--end

