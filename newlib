			Icon_5.Image = "rbxassetid://3926305904"
			Icon_5.ImageColor3 = Color3.fromRGB(125, 125, 125)
			Icon_5.ImageRectOffset = Vector2.new(724, 444)
			Icon_5.ImageRectSize = Vector2.new(36, 36)

			OutlineShadow_5.Name = "Outline Shadow"
			OutlineShadow_5.Parent = Keybind
			OutlineShadow_5.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			OutlineShadow_5.BackgroundTransparency = 1.000
			OutlineShadow_5.Position = UDim2.new(0, -15, 0, -15)
			OutlineShadow_5.Selectable = true
			OutlineShadow_5.Size = UDim2.new(1, 30, 1, 30)
