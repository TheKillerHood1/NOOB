				--Container.BackgroundTransparency = 0.95	
				TS:Create(Container_5, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()
			end)

			Container_5.MouseLeave:Connect(function()
				--Container.BackgroundTransparency = 1
				TS:Create(Container_5, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()
			end)

			local keybindtoggle = false
			TextButton_4.MouseButton1Click:Connect(function()
				TS:Create(Container_5, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()	
				wait()
				TS:Create(Container_5, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()

				if keybindtoggle == false then
					keybindtoggle = true
					TextLabel_2.Text = ". . ."
					Thing = game:GetService("UserInputService").InputBegan:Connect(function(Key, IsNotFocused)
						if IsNotFocused then return end
						if Key.UserInputType == Enum.UserInputType.Keyboard then
							TextLabel_2.Text = Key.KeyCode.Name
							callback(Key.KeyCode.Name)
							keybindtoggle = false
							Thing:Disconnect()
						else
							TextLabel_2.Text = "Unknown"
							Thing:Disconnect()
						end
					end)
				else
					keybindtoggle = false
				end
			end)
			--END
		end

