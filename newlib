				Space_2.TextSize = 14.000


				TextLabel_22.Parent = top
				TextLabel_22.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_22.BackgroundTransparency = 1.000
				TextLabel_22.Position = UDim2.new(0.580796242, 0, 0, 0)
				TextLabel_22.Size = UDim2.new(0, 136, 0, 28)
				TextLabel_22.Font = Enum.Font.Gotham
				TextLabel_22.Text = "Selected: None"
				TextLabel_22.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_22.TextScaled = true
				TextLabel_22.TextSize = 16.000
				TextLabel_22.TextWrapped = true

				for i,v in pairs(options) do
					local TextButton_2 = Instance.new("TextButton")
					local UICorner_5 = Instance.new("UICorner")
					local TextLabel_4 = Instance.new("TextLabel")

					TextButton_2.Parent = ScrollingFrame
					TextButton_2.BackgroundColor3 = Color3.fromRGB(99, 98, 100)
					TextButton_2.Position = UDim2.new(-0.00592417084, 0, 0.263999999, 0)
					TextButton_2.Size = UDim2.new(0, 370, 0, 28)
					TextButton_2.Font = Enum.Font.SourceSans
					TextButton_2.Text = ""
					TextButton_2.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextButton_2.TextSize = 14.000
					TextButton_2.MouseButton1Down:Connect(function()		
						TextLabel_22.Text = "Selected: " .. v
						callback(v)
					end)

					UICorner_5.CornerRadius = UDim.new(0, 5)
					UICorner_5.Parent = TextButton_2

					TextLabel_4.Parent = TextButton_2
					TextLabel_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					TextLabel_4.BackgroundTransparency = 1.000
					TextLabel_4.Position = UDim2.new(-0.00252553588, 0, 0.0357142873, 0)
					TextLabel_4.Size = UDim2.new(0, 369, 0, 25)
					TextLabel_4.Font = Enum.Font.Gotham
					TextLabel_4.Text = v
					TextLabel_4.TextColor3 = Color3.fromRGB(255, 255, 255)
					TextLabel_4.TextSize = 16.000
					TextLabel_4.TextWrapped = true
				end
			end


			return buttons
		end
		return mysection
	end

	function mypage:CreateChat()
		local Chat = Instance.new("Frame")
		local ScrollingFrame = Instance.new("ScrollingFrame")
		local UIListLayout = Instance.new("UIListLayout")
		local space = Instance.new("Frame")
		local Frame = Instance.new("Frame")
		local TextBox = Instance.new("TextBox")
		local UICorner_6 = Instance.new("UICorner")
		local send = Instance.new("TextButton")
		local UICorner_7 = Instance.new("UICorner")

		Chat.Name = "ChatPage"
		Chat.Parent = Holder
		Chat.Active = true
		Chat.BackgroundColor3 = Color3.fromRGB(113, 113, 113)
		Chat.BackgroundTransparency = 1.000
		Chat.BorderColor3 = Color3.fromRGB(135, 135, 135)
		Chat.BorderSizePixel = 0
		Chat.Position = UDim2.new(0, 0, 0.0865384638, 0)
		Chat.Size = UDim2.new(0, 444, 0, 380)
		Chat.ZIndex = 0
		--Chat.ScrollBarThickness = 0

		if firstpage == false then
			firstpage = true
			Chat.Visible = true
		else
			Chat.Visible = false
		end

		ScrollingFrame.Parent = Chat
		ScrollingFrame.Active = true
		ScrollingFrame.BackgroundColor3 = Color3.fromRGB(39, 39, 39)
		ScrollingFrame.Size = UDim2.new(0, 444, 0, 310)

		UIListLayout.Parent = ScrollingFrame
		UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.Padding = UDim.new(0, 10)

		local function ResizeFrame() 
			--tab.CanvasSize = tab.CanvasSize + UDim2.new(0,0,0,44)
			ScrollingFrame.CanvasSize = UDim2.new(0,0,0,UIListLayout.AbsoluteContentSize.Y + 10)
		end

		ScrollingFrame.ChildAdded:Connect(function()
			wait()
			--for i = 1,5 do
			ScrollingFrame.CanvasPosition = ScrollingFrame.CanvasPosition + Vector2.new(0,90000000)
			--end
		end)


		game:service"RunService".RenderStepped:connect(function()
			ResizeFrame() 
		end)

		space.Name = "space"
		space.Parent = ScrollingFrame
		space.BackgroundTransparency = 1.000
		space.Position = UDim2.new(0.0540540554, 0, 0, 0)
		space.Size = UDim2.new(0, 396, 0, 0)

		Frame.Parent = Chat
		Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Frame.BackgroundTransparency = 1.000
		Frame.Position = UDim2.new(0, 0, 0, 317)
		Frame.Size = UDim2.new(0, 444, 0, 63)

		TextBox.Parent = Frame
		TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextBox.Position = UDim2.new(0.018018024, 0, 0, 0)
		TextBox.Size = UDim2.new(0, 288, 0, 50)
		TextBox.Font = Enum.Font.Gotham
		TextBox.PlaceholderText = "Type a message..."
		TextBox.Text = ""
		TextBox.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextBox.TextScaled = true
		TextBox.TextSize = 14.000
		TextBox.TextWrapped = true

		UICorner_6.Parent = TextBox

		send.Name = "send"
		send.Parent = Frame
		send.BackgroundColor3 = Color3.fromRGB(0, 157, 0)
		send.Position = UDim2.new(0.695945919, 0, 0, 0)
		send.Size = UDim2.new(0, 110, 0, 50)
		send.Font = Enum.Font.GothamBold
		send.Text = "Send"
		send.TextColor3 = Color3.fromRGB(0, 0, 0)
		send.TextScaled = true
		send.TextSize = 14.000
		send.TextWrapped = true


		UICorner_7.Parent = send

		if syn and not _G.work then
			local json_encode = function(tbl)
				local json = game:GetService("HttpService"):JSONEncode(tbl)
				return json
			end
			local json_decode = function(json)
				local tbl = game:GetService("HttpService"):JSONDecode(json)
				return tbl
			end

			_G.work = syn.websocket.connect('wss://steepvitalcases.badguy51384.repl.co/')
			local web = _G.work

			function sendMessage(msg)
				local newmsg = {
					['Username'] = game.Players.LocalPlayer.Name,
					['Message'] = msg .. "|" .. game.PlaceId
				}	
				newmsg = json_encode(newmsg)
				web:Send(newmsg)
			end

			local cooldown = false
			send.MouseButton1Down:Connect(function()
				if TextBox.Text ~="" and cooldown == false then
					cooldown = true
					--web:Send(TextBox.Text)
					sendMessage(TextBox.Text)
					wait(3)
					cooldown = false
				end
			end)

			_G.work:Send(json_encode({
				['Action'] = 'Connected',
				['Username'] = game.Players.LocalPlayer.Name .. "|" .. game.PlaceId,
			}))

			function messageinchat(msg)
				local messageframe = Instance.new("Frame")
				local ImageLabel = Instance.new("ImageLabel")
				local UICorner_3 = Instance.new("UICorner")
				local UICorner_4 = Instance.new("UICorner")
				local TextLabel_3 = Instance.new("TextLabel")
				local UICorner_5 = Instance.new("UICorner")

				messageframe.Name = "messageframe"
				messageframe.Parent = ScrollingFrame
				messageframe.Size = UDim2.new(0, 396, 0, 65)
				messageframe.BackgroundTransparency = 1

				ImageLabel.Parent = messageframe
				ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				ImageLabel.Position = UDim2.new(0.0405405201, 0, 0.0923076943, 0)
				ImageLabel.Size = UDim2.new(0, 60, 0, 53)
				ImageLabel.BackgroundTransparency = 1
				ImageLabel.Image = "rbxassetid://7672064766"

				UICorner_3.Parent = ImageLabel

				UICorner_4.CornerRadius = UDim.new(0, 5)
				UICorner_4.Parent = messageframe

				TextLabel_3.Parent = messageframe
				TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_3.Position = UDim2.new(0.247474745, 0, 0.107692309, 0)
				TextLabel_3.Size = UDim2.new(0, 288, 0, 50)
				TextLabel_3.Font = Enum.Font.Gotham
				TextLabel_3.Text = msg
				TextLabel_3.TextColor3 = Color3.fromRGB(0, 0, 0)
				TextLabel_3.TextScaled = true
				TextLabel_3.TextSize = 14.000
				TextLabel_3.TextWrapped = true
				TextLabel_3.TextXAlignment = Enum.TextXAlignment.Left

				UICorner_5.Parent = TextLabel_3
			end


			_G.work.OnMessage:Connect(function(msg)
				local data = json_decode(msg)
				if data['Action'] == 'New User' then
				local split = data.Username:split('|')
                local msg = split[1]
                local gameid = split[2]
					game.StarterGui:SetCore("SendNotification", {
						Title = "Killer-Lab"; -- the title (ofc)
						Text = "Someone has joined the chat!"; -- what the text says (ofc)
						Duration = 3; -- how long the notification should in secounds
					})
					if tonumber(gameid) == game.PlaceId then
					messageinchat("Someone has joined the chat!")
					end
					return
				end
				if data.Message then
			--	print(data.Message)
				local split = data.Message:split('|')
                local msg = split[1]
                local gameid = split[2]
                if tonumber(gameid) == game.PlaceId then
					print("work")
					messageinchat(msg)
					--messageinchat(data.Message)
			    	end
				end
			end)

		end

		--new message

		--[[
		local messageframe = Instance.new("Frame")
		local ImageLabel = Instance.new("ImageLabel")
		local UICorner_3 = Instance.new("UICorner")
		local UICorner_4 = Instance.new("UICorner")
		local TextLabel_3 = Instance.new("TextLabel")
		local UICorner_5 = Instance.new("UICorner")
		
		messageframe.Name = "messageframe"
		messageframe.Parent = ScrollingFrame
		messageframe.Size = UDim2.new(0, 396, 0, 65)

		ImageLabel.Parent = messageframe
		ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ImageLabel.Position = UDim2.new(0.0405405201, 0, 0.0923076943, 0)
		ImageLabel.Size = UDim2.new(0, 60, 0, 53)
		ImageLabel.Image = "rbxassetid://7672064766"

		UICorner_3.Parent = ImageLabel

		UICorner_4.CornerRadius = UDim.new(0, 5)
		UICorner_4.Parent = messageframe

		TextLabel_3.Parent = messageframe
		TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel_3.Position = UDim2.new(0.247474745, 0, 0.107692309, 0)
		TextLabel_3.Size = UDim2.new(0, 288, 0, 50)
		TextLabel_3.Font = Enum.Font.Gotham
		TextLabel_3.Text = "nothing"
		TextLabel_3.TextColor3 = Color3.fromRGB(0, 0, 0)
		TextLabel_3.TextScaled = true
		TextLabel_3.TextSize = 14.000
		TextLabel_3.TextWrapped = true
		TextLabel_3.TextXAlignment = Enum.TextXAlignment.Left

		UICorner_5.Parent = TextLabel_3]]

		--end

	end

	return mypage	
end
