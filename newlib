				TextLabel_7.Position = UDim2.new(0.0163934417, 0, 0.0357140079, 0)
				TextLabel_7.Size = UDim2.new(0, 364, 0, 25)
				TextLabel_7.Font = Enum.Font.Gotham
				TextLabel_7.Text = name .. min
				TextLabel_7.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_7.TextSize = 16.000
