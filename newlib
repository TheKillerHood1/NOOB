				TextLabel_6.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_6.BackgroundTransparency = 1.000
				TextLabel_6.Position = UDim2.new(0.00908494741, 0, 0.144452885, 0)
				TextLabel_6.Size = UDim2.new(0, 120, 0, 22)
				TextLabel_6.Font = Enum.Font.SourceSans
				TextLabel_6.Text = name
				TextLabel_6.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_6.TextScaled = true
				TextLabel_6.TextSize = 18.000
				TextLabel_6.TextWrapped = true
				TextLabel_6.TextXAlignment = Enum.TextXAlignment.Left	
				
				local keybindtoggle = false
				TextButton.MouseButton1Down:Connect(function()
					if keybindtoggle == false then
						keybindtoggle = true
						TextLabel_5.Text = ". . ."
						Thing = game:GetService("UserInputService").InputBegan:Connect(function(Key, IsNotFocused)
							if IsNotFocused then return end
							if Key.UserInputType == Enum.UserInputType.Keyboard then
								TextLabel_5.Text = Key.KeyCode.Name
								callback(Key.KeyCode.Name)
								nowbutton = Key.KeyCode.Name
								keybindtoggle = false
								Thing:Disconnect()
							else
								TextLabel_5.Text = "Unknown"
								Thing:Disconnect()
							end
						end)
					else
						keybindtoggle = false
					end
				end)
				
				game:GetService("UserInputService").InputBegan:Connect(function(Input, IsTyping)
					if IsTyping then return end
					if Input.KeyCode == Enum.KeyCode[nowbutton] then
						randomframe.Visible = not randomframe.Visible
						Holder.Visible = not Holder.Visible



						--MOVING MOUSE FUNCTION
						local LocalPlayer = game:GetService('Players').LocalPlayer;
						local Mouse = LocalPlayer:GetMouse();
						local InputService = game:GetService('UserInputService');
						local RunService = game:GetService('RunService')
						local RenderStepped = RunService.RenderStepped;
						
						local oIcon = Mouse.Icon;
						local State = InputService.MouseIconEnabled;
					
						local Cursor = Drawing.new('Triangle');
						Cursor.Thickness = 1;
						Cursor.Filled = true;
						

						while Holder.Visible do
							local mPos = workspace.CurrentCamera:WorldToViewportPoint(Mouse.Hit.p);

							Cursor.Color = Color3.fromRGB(121, 121, 121);
							Cursor.PointA = Vector2.new(mPos.X, mPos.Y);
							Cursor.PointB = Vector2.new(mPos.X, mPos.Y) + Vector2.new(6, 14);
							Cursor.PointC = Vector2.new(mPos.X, mPos.Y) + Vector2.new(-6, 14);

							Cursor.Visible = not InputService.MouseIconEnabled;

							RenderStepped:Wait();
						end;
						--END
						--DELETE WHEN DONE
						Cursor:Remove();
						--END
					end
				end)		
			end
			
