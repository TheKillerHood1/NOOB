	local mypage = {}

	function mypage:CreatePage(name)


		--create main
		local Main = Instance.new("ScrollingFrame")
		local UIListLayout = Instance.new("UIListLayout")
		local Space = Instance.new("Frame")

		Main.Name = "Main"
		Main.Parent = Holder
		Main.Active = true
		Main.BackgroundColor3 = Color3.fromRGB(113, 113, 113)
		Main.BackgroundTransparency = 1.000
		Main.BorderColor3 = Color3.fromRGB(135, 135, 135)
		Main.BorderSizePixel = 0
		Main.Position = UDim2.new(0, 0, 0.0865384638, 0)
		Main.Size = UDim2.new(0, 444, 0, 380)
		Main.ScrollBarThickness = 0
		Main.ZIndex = 0
		if firstpage == false then
			firstpage = true
			Main.Visible = true
		else
			Main.Visible = false
		end

		local chatvis = false
		chat.MouseButton1Down:Connect(function()
			if chatvis == false then
				chatvis = true
				Main.Visible = false
				lastopened = Main
				Holder.ChatPage.Visible = true
			else
				chatvis = false
				lastopened.Visible = true
				Holder.ChatPage.Visible = false
			end
		end)

		UIListLayout.Parent = Main
		UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		UIListLayout.Padding = UDim.new(0, 5)

		local function ResizeFrame() 
			--tab.CanvasSize = tab.CanvasSize + UDim2.new(0,0,0,44)
			Main.CanvasSize = UDim2.new(0,0,0,UIListLayout.AbsoluteContentSize.Y + 10)
		end

		game:service"RunService".RenderStepped:connect(function()
			ResizeFrame() 
		end)

		Space.Name = "Space"
		Space.Parent = Main
		Space.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Space.BackgroundTransparency = 1.000
		Space.ClipsDescendants = true
		Space.Size = UDim2.new(0, 444, 0, 6)


		--end

		--create pages button for main
		local pagebutton = Instance.new("TextButton")
		local UICorner_7 = Instance.new("UICorner")
		local TextLabel_4 = Instance.new("TextLabel")

		pagebutton.Name = "pagebutton"
		pagebutton.Parent = Pages
		pagebutton.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
		pagebutton.Position = UDim2.new(0.0849673226, 0, 0, 0)
		pagebutton.Size = UDim2.new(0, 127, 0, 28)
		pagebutton.Font = Enum.Font.Gotham
		pagebutton.Text = ""
		pagebutton.TextColor3 = Color3.fromRGB(227, 225, 228)
		pagebutton.TextSize = 12.000
		pagebutton.TextStrokeColor3 = Color3.fromRGB(163, 162, 165)
		pagebutton.TextWrapped = true
		pagebutton.TextXAlignment = Enum.TextXAlignment.Left
		pagebutton.MouseButton1Down:Connect(function()
			for i,v in pairs(Holder:GetChildren()) do
				if v.Name ~= "Top" and v.Name ~= "SettingsFrame" and v.Name ~= "UICorner" and v.Name ~= "4pxShadow(2px)" then
					v.Visible = false
					Main.Visible = true

				end
			end
			if toggle == true then
				settingstoggle = false
				TS:Create(SettingsFrame, TweenInfo.new(tweenspeed), {Size = UDim2.new(0, 0,0, 416)}):Play()
				print("WORKED")
			end
		end)

		UICorner_7.CornerRadius = UDim.new(0, 5)
		UICorner_7.Parent = pagebutton

		TextLabel_4.Parent = pagebutton
		TextLabel_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel_4.BackgroundTransparency = 1.000
		TextLabel_4.Position = UDim2.new(0.0472440943, 0, 0.110644266, 0)
		TextLabel_4.Size = UDim2.new(0, 121, 0, 21)
		TextLabel_4.Font = Enum.Font.Gotham
		TextLabel_4.Text = name
		TextLabel_4.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel_4.TextScaled = true
		TextLabel_4.TextSize = 14.000
		TextLabel_4.TextWrapped = true
		TextLabel_4.TextXAlignment = Enum.TextXAlignment.Left
		--end

