			OutlineShadow_5.ImageColor3 = Color3.fromRGB(138, 138, 138)
			OutlineShadow_5.ImageTransparency = 0.700
			OutlineShadow_5.ScaleType = Enum.ScaleType.Slice
			OutlineShadow_5.SliceCenter = Rect.new(17, 17, 283, 283)

