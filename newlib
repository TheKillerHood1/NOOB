				UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
				UIListLayout_3.Padding = UDim.new(0, 5)

				local function ResizeFrame()                              --Function to resize the frame
					ScrollingFrame.CanvasSize = UDim2.new(0,0,0,UIListLayout_3.AbsoluteContentSize.Y + 10)
				end

				game:service"RunService".RenderStepped:connect(function()
					ResizeFrame() 
				end)

				Space_2.Name = "Space"
				Space_2.Parent = ScrollingFrame
				Space_2.BackgroundColor3 = Color3.fromRGB(99, 98, 100)
				Space_2.BackgroundTransparency = 1.000
				Space_2.Position = UDim2.new(-0.00592417084, 0, 0, 0)
				Space_2.Size = UDim2.new(0, 427, 0, 2)
				Space_2.ZIndex = 0
				Space_2.Font = Enum.Font.SourceSans
				Space_2.Text = ""
				Space_2.TextColor3 = Color3.fromRGB(0, 0, 0)
				Space_2.TextSize = 14.000


				TextLabel_22.Parent = top
				TextLabel_22.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_22.BackgroundTransparency = 1.000
				TextLabel_22.Position = UDim2.new(0.580796242, 0, 0, 0)
				TextLabel_22.Size = UDim2.new(0, 136, 0, 28)
				TextLabel_22.Font = Enum.Font.Gotham
				TextLabel_22.Text = "Selected: None"
				TextLabel_22.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextLabel_22.TextScaled = true
				TextLabel_22.TextSize = 16.000
				TextLabel_22.TextWrapped = true

				local inmydropdown = {}
				for i,v in pairs(options) do
					local TextButton_2 = Instance.new("TextButton")
					local UICorner_5 = Instance.new("UICorner")
					local TextLabel_4 = Instance.new("TextLabel")

					TextButton_2.Parent = ScrollingFrame
					TextButton_2.BackgroundColor3 = Color3.fromRGB(99, 98, 100)
					TextButton_2.Position = UDim2.new(-0.00592417084, 0, 0.263999999, 0)
					TextButton_2.Size = UDim2.new(0, 370, 0, 28)
					TextButton_2.Font = Enum.Font.SourceSans
					TextButton_2.Text = ""
					TextButton_2.TextColor3 = Color3.fromRGB(0, 0, 0)
					TextButton_2.TextSize = 14.000
					TextButton_2.MouseButton1Down:Connect(function()		
						if table.find(inmydropdown,v) then
							table.remove(inmydropdown,inmydropdown[v])
						else
							table.insert(inmydropdown,v)
						end
						TextLabel_22.Text = "Selected: " .. table.concat(inmydropdown, ", ")
						--callback(table.concat(inmydropdown, ","))
						callback(v)
					end)

					UICorner_5.CornerRadius = UDim.new(0, 5)
					UICorner_5.Parent = TextButton_2

					TextLabel_4.Parent = TextButton_2
					TextLabel_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					TextLabel_4.BackgroundTransparency = 1.000
					TextLabel_4.Position = UDim2.new(-0.00252553588, 0, 0.0357142873, 0)
					TextLabel_4.Size = UDim2.new(0, 369, 0, 25)
					TextLabel_4.Font = Enum.Font.Gotham
					TextLabel_4.Text = v
					TextLabel_4.TextColor3 = Color3.fromRGB(255, 255, 255)
					TextLabel_4.TextSize = 16.000
					TextLabel_4.TextWrapped = true
				end
			end

