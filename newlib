local TS = game:GetService("TweenService")
local UIS = game:GetService("UserInputService")

local library = {}

function library:CreateWindow(name, name1)
	name = name or "Universe Hub | V1"
	name1 = name1 or "Sections"

	--CREATE MAIN WINDOW
	local ScreenGui = Instance.new("ScreenGui")
	local Main = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local _4pxShadow2px = Instance.new("ImageLabel")

	ScreenGui.Parent = game.CoreGui
	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Main.Name = "Main"
	Main.Parent = ScreenGui
	Main.BackgroundColor3 = Color3.fromRGB(49, 49, 59)
	Main.Position = UDim2.new(0, 653, 0, 298) -- OLD POSITION = 0, 499, 0, 191 NEW POSITION = 0, 653, 0, 298
	Main.Size = UDim2.new(0, 583, 0, 307)
