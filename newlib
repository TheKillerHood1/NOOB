			ValueText.Font = Enum.Font.Gotham
			ValueText.Text = "16"
			ValueText.TextColor3 = Color3.fromRGB(227, 225, 228)
			ValueText.TextSize = 12.000

			OutlineShadow_6.Name = "Outline Shadow"
			OutlineShadow_6.Parent = Slider
			OutlineShadow_6.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			OutlineShadow_6.BackgroundTransparency = 1.000
			OutlineShadow_6.Position = UDim2.new(0, -15, 0, -15)
			OutlineShadow_6.Selectable = true
			OutlineShadow_6.Size = UDim2.new(1, 30, 1, 30)
			OutlineShadow_6.Image = "http://www.roblox.com/asset/?id=5761504593"
			OutlineShadow_6.ImageColor3 = Color3.fromRGB(138, 138, 138)
			OutlineShadow_6.ImageTransparency = 0.700
			OutlineShadow_6.ScaleType = Enum.ScaleType.Slice
			OutlineShadow_6.SliceCenter = Rect.new(17, 17, 283, 283)
			--END


			Slider.MouseEnter:Connect(function()
				--Container.BackgroundTransparency = 0.95	
				TS:Create(Slider, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()
			end)

			Slider.MouseLeave:Connect(function()
				--Container.BackgroundTransparency = 1
				TS:Create(Slider, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()
			end)



			local value
			local dragging
			local SliderBar = Tracker
			local Sliderbutton = TextButton_5
			local Slider_2 = Indicator_3
			function library4:SetValue(input)
				local pos = UDim2.new(math.clamp((input.Position.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1), 0, 0, (SliderBar.AbsoluteSize.Y))
				Slider_2:TweenSize(pos, Enum.EasingDirection.Out, Enum.EasingStyle.Quad, 0.2, true)
				local value = math.floor(( ((pos.X.Scale * max) / max) * (max - min) + min ) * 100) / 100
				ValueText.Text = tostring(value)
				library4["Value"] = value
				spawn(function() callback(value) wait() ValueText.Text = value end)
			end;

			Sliderbutton.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = true
					TS:Create(Fade,TweenInfo.new(.1),{BackgroundTransparency=0.8}):Play()
				end
			end)

			Sliderbutton.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = false
					TS:Create(Fade,TweenInfo.new(.1),{BackgroundTransparency=1}):Play()
				end
			end)

			Sliderbutton.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					library4:SetValue(input)
				end
			end)

			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
					--print(input)
					library4:SetValue(input)
				end
			end)
			--return library4	
		end

