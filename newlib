	local pages = {}
	function pages:CreateTab(name)
		name = name or "Sections"

		--CREATE BUTTON AT MENU
		local PageButton = Instance.new("TextButton")
		local Indicator = Instance.new("Frame")

		PageButton.Name = "PageButton"
		PageButton.Parent = Menu
