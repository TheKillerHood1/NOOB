			Icon_2.BackgroundTransparency = 1.000
			Icon_2.Position = UDim2.new(0.0144927306, 0, 0.189655393, 0)
			Icon_2.Size = UDim2.new(0, 18, 0, 18)
			Icon_2.ZIndex = 2
			Icon_2.Image = "rbxassetid://3926305904"
			Icon_2.ImageColor3 = Color3.fromRGB(125, 125, 125)
			Icon_2.ImageRectOffset = Vector2.new(764, 84)
			Icon_2.ImageRectSize = Vector2.new(36, 36)

			OutlineShadow_2.Name = "Outline Shadow"
			OutlineShadow_2.Parent = Toggle
			OutlineShadow_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			OutlineShadow_2.BackgroundTransparency = 1.000
			OutlineShadow_2.Position = UDim2.new(0, -15, 0, -15)
			OutlineShadow_2.Selectable = true
			OutlineShadow_2.Size = UDim2.new(1, 30, 1, 30)
			OutlineShadow_2.Image = "http://www.roblox.com/asset/?id=5761504593"
			OutlineShadow_2.ImageColor3 = Color3.fromRGB(138, 138, 138)
			OutlineShadow_2.ImageTransparency = 0.700
			OutlineShadow_2.ScaleType = Enum.ScaleType.Slice
			OutlineShadow_2.SliceCenter = Rect.new(17, 17, 283, 283)

			Container_2.MouseEnter:Connect(function()
				--Container.BackgroundTransparency = 0.95	
				TS:Create(Container_2, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()
			end)

			Container_2.MouseLeave:Connect(function()
				--Container.BackgroundTransparency = 1
				TS:Create(Container_2, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()
			end)

			local mytoggle = false
			TextButton_2.MouseButton1Down:Connect(function()
				TS:Create(Container_2, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()	
				wait()
				TS:Create(Container_2, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()
				if mytoggle == false then
					mytoggle = true
					callback(true)
					TS:Create(Dot, TweenInfo.new(0.5), {BackgroundTransparency = 0}):Play()
				elseif mytoggle == true then
					mytoggle = false
					callback(false)
					TS:Create(Dot, TweenInfo.new(0.5), {BackgroundTransparency = 1}):Play()
				end
			end)
			--END
		end
		
		function buttons:CreateDropDown(name, options, callback)
			name = name or "DropDown"
			options = options or "Name"
			callback = callback or function() end
			
			
			--CREATE DROPDOWN
			local DropDown = Instance.new("Frame")
			local Frame = Instance.new("Frame")
			local Icon_6 = Instance.new("ImageButton")
			local TextButton_6 = Instance.new("TextButton")
			local ScrollingFrame = Instance.new("ScrollingFrame")
			local UIListLayout_3 = Instance.new("UIListLayout")
			local UIPadding_2 = Instance.new("UIPadding")
			local OutlineShadow_8 = Instance.new("ImageLabel")
			local TextLabel_3 = Instance.new("TextLabel")

			
			DropDown.Name = "DropDown"
			DropDown.Parent = Page
			DropDown.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
			DropDown.BackgroundTransparency = 1.000
			DropDown.ClipsDescendants = true
			DropDown.Position = UDim2.new(-0.00483696256, 0, 0.17786561, 0)
			DropDown.Size = UDim2.new(1.00999999, 0, -0, 30)

			Frame.Parent = DropDown
			Frame.BackgroundColor3 = Color3.fromRGB(40, 40, 48)
			Frame.BackgroundTransparency = 0
			Frame.Position = UDim2.new(-0.00567660062, 0, 0, 0)
			Frame.Size = UDim2.new(0, 414, 0, 30)

			Icon_6.Name = "Icon"
			Icon_6.Parent = Frame
			Icon_6.BackgroundTransparency = 1.000
			Icon_6.Position = UDim2.new(0.0144927502, 0, 0.189655185, 0)
			Icon_6.Rotation = -90.000
			Icon_6.Size = UDim2.new(0, 18, 0, 18)
			Icon_6.ZIndex = 2
			Icon_6.Image = "rbxassetid://3926307971"
			Icon_6.ImageColor3 = Color3.fromRGB(125, 125, 125)
			Icon_6.ImageRectOffset = Vector2.new(204, 484)
			Icon_6.ImageRectSize = Vector2.new(36, 36)

			TextButton_6.Parent = Frame
			TextButton_6.BackgroundTransparency = 0.990
			TextButton_6.Position = UDim2.new(0, 30, 0, -1)
			TextButton_6.Size = UDim2.new(1, -30, 1, 0)
			TextButton_6.ZIndex = 2
			TextButton_6.Font = Enum.Font.SourceSans
			TextButton_6.Text = name
			TextButton_6.TextColor3 = Color3.fromRGB(255, 255, 255)
			TextButton_6.TextSize = 14.000
			TextButton_6.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
			TextButton_6.TextWrapped = true
			TextButton_6.TextXAlignment = Enum.TextXAlignment.Left

			ScrollingFrame.Parent = DropDown
			ScrollingFrame.Active = true
			ScrollingFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 36)
			ScrollingFrame.BorderSizePixel = 0
			ScrollingFrame.Position = UDim2.new(0, 0, 0, 30)
			ScrollingFrame.Size = UDim2.new(1.07193542, -30, 0.642013073, 0)
			ScrollingFrame.CanvasSize = UDim2.new(0, 0, 1, 0)

			UIListLayout_3.Parent = ScrollingFrame
			UIListLayout_3.HorizontalAlignment = Enum.HorizontalAlignment.Center
			UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
			UIListLayout_3.Padding = UDim.new(0, 8)

			UIPadding_2.Parent = ScrollingFrame
			UIPadding_2.PaddingBottom = UDim.new(0, 4)
			UIPadding_2.PaddingLeft = UDim.new(0, 4)
			UIPadding_2.PaddingRight = UDim.new(0, 4)
			UIPadding_2.PaddingTop = UDim.new(0, 8)
			
			OutlineShadow_8.Name = "Outline Shadow"
			OutlineShadow_8.Parent = DropDown
			OutlineShadow_8.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			OutlineShadow_8.BackgroundTransparency = 1.000
			OutlineShadow_8.Position = UDim2.new(0, -14, 0, -13)
			OutlineShadow_8.Selectable = true
			OutlineShadow_8.Size = UDim2.new(0.989245355, 30, 0.76433152, 30)
			OutlineShadow_8.Image = "http://www.roblox.com/asset/?id=5761504593"
			OutlineShadow_8.ImageColor3 = Color3.fromRGB(138, 138, 138)
			OutlineShadow_8.ImageTransparency = 0.700
			OutlineShadow_8.ScaleType = Enum.ScaleType.Slice
			OutlineShadow_8.SliceCenter = Rect.new(17, 17, 283, 283)
			
			TextLabel_3.Parent = Frame
			TextLabel_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel_3.BackgroundTransparency = 1.000
			TextLabel_3.Position = UDim2.new(0.816999972, 0, 0.25, -3)
			TextLabel_3.Size = UDim2.new(0, 58, 0, 17)
			TextLabel_3.ZIndex = 5
			TextLabel_3.Font = Enum.Font.SourceSans
			TextLabel_3.Text = "Selected:"
			TextLabel_3.TextColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel_3.TextSize = 14.000
			--END
			
			for i,v in pairs(options) do
				--CREATE BUTTONS
				local Button_2 = Instance.new("Frame")
				local Container_6 = Instance.new("Frame")
				local UICorner_22 = Instance.new("UICorner")
				local TextButton_7 = Instance.new("TextButton")
				local Icon_7 = Instance.new("ImageButton")
				local OutlineShadow_7 = Instance.new("ImageLabel")
				
				Button_2.Name = "Button"
				Button_2.Parent = ScrollingFrame
				Button_2.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
				Button_2.BackgroundTransparency = 1.000
				Button_2.Position = UDim2.new(0.0241554249, 0, 0.00990098994, 0)
				Button_2.Size = UDim2.new(0.951689124, 0, -0.00990098994, 30)

				Container_6.Name = "Container"
				Container_6.Parent = Button_2
				Container_6.BackgroundColor3 = Color3.fromRGB(40, 40, 48)
				Container_6.BorderSizePixel = 0
				Container_6.Position = UDim2.new(1.58049446e-07, 0, 0.00744883204, 0)
				Container_6.Size = UDim2.new(0.999999821, 0, 1.03492391, -1)
				Container_6.ZIndex = 2
				
				Container_6.MouseEnter:Connect(function()
					--Container.BackgroundTransparency = 0.95	
					TS:Create(Container_6, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()
				end)

				Container_6.MouseLeave:Connect(function()
					--Container.BackgroundTransparency = 1
					TS:Create(Container_6, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()
				end)

				UICorner_22.CornerRadius = UDim.new(0, 4)
				UICorner_22.Parent = Container_6

				TextButton_7.Parent = Container_6
				TextButton_7.BackgroundTransparency = 0.990
				TextButton_7.Position = UDim2.new(0, 30, 0, 0)
				TextButton_7.Size = UDim2.new(1, -30, 1, 0)
				TextButton_7.Font = Enum.Font.SourceSans
				TextButton_7.Text = v
				TextButton_7.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextButton_7.TextSize = 14.000
				TextButton_7.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
				TextButton_7.TextWrapped = true
				TextButton_7.TextXAlignment = Enum.TextXAlignment.Left
				TextButton_7.MouseButton1Down:Connect(function()
					TextLabel_3.Text = "Selected: " .. v
					callback(v)
				end)

				Icon_7.Name = "Icon"
				Icon_7.Parent = Container_6
				Icon_7.BackgroundTransparency = 1.000
				Icon_7.Position = UDim2.new(0.0144927502, 0, 0.189655185, 0)
				Icon_7.Size = UDim2.new(0, 18, 0, 18)
				Icon_7.ZIndex = 2
				Icon_7.Image = "rbxassetid://3926305904"
				Icon_7.ImageColor3 = Color3.fromRGB(125, 125, 125)
				Icon_7.ImageRectOffset = Vector2.new(84, 204)
				Icon_7.ImageRectSize = Vector2.new(36, 36)

				OutlineShadow_7.Name = "Outline Shadow"
				OutlineShadow_7.Parent = Button_2
				OutlineShadow_7.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				OutlineShadow_7.BackgroundTransparency = 1.000
				OutlineShadow_7.Position = UDim2.new(0, -15, 0, -15)
				OutlineShadow_7.Selectable = true
				OutlineShadow_7.Size = UDim2.new(1, 30, 1, 30)
				OutlineShadow_7.Image = "http://www.roblox.com/asset/?id=5761504593"
				OutlineShadow_7.ImageColor3 = Color3.fromRGB(138, 138, 138)
				OutlineShadow_7.ImageTransparency = 0.700
				OutlineShadow_7.ScaleType = Enum.ScaleType.Slice
				OutlineShadow_7.SliceCenter = Rect.new(17, 17, 283, 283)
				--END
				
				
				
			end

			
			--FUNCTIONS
			Frame.MouseEnter:Connect(function()
				--Container.BackgroundTransparency = 0.95	
				TS:Create(Frame, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(45, 45, 54)}):Play()
			end)

			Frame.MouseLeave:Connect(function()
				--Container.BackgroundTransparency = 1
				TS:Create(Frame, TweenInfo.new(0.5), {BackgroundColor3 = Color3.fromRGB(40, 40, 48)}):Play()
			end)
			
			local function ResizeFrame()                              --Function to resize the frame
				--tab.CanvasSize = tab.CanvasSize + UDim2.new(0,0,0,44)
				ScrollingFrame.CanvasSize = UDim2.new(0,0,0,UIListLayout_3.AbsoluteContentSize.Y + 20)
			end

			game:service"RunService".RenderStepped:connect(function()
				ResizeFrame() 
			end)
			
			local dropdowntoggle = false
			TextButton_6.MouseButton1Down:Connect(function()
				if dropdowntoggle == false then
					dropdowntoggle = true
					TS:Create(Icon_6, TweenInfo.new(0.2), {Rotation = 0}):Play()
					TS:Create(DropDown, TweenInfo.new(0.2), {Size = UDim2.new(1.01, 0,0.38, 30)}):Play()
					TS:Create(OutlineShadow_8, TweenInfo.new(0.2), {Size = UDim2.new(0.989, 30,0.785, 30)}):Play()
				elseif dropdowntoggle == true then
					dropdowntoggle = false	
					TS:Create(Icon_6, TweenInfo.new(0.2), {Rotation = -90}):Play()
					TS:Create(DropDown, TweenInfo.new(0.2), {Size = UDim2.new(1.01, 0,-0, 30)}):Play()
					TS:Create(OutlineShadow_8, TweenInfo.new(0.2), {Size = UDim2.new(0.989245355, 30, 0.76433152, 30)}):Play()
				end
			end)
			--END
			
		end

		return buttons
	end
	return pages
end
return library

--[[dropdownbutton
local dropdowntoggle = false
TextButton_6.MouseButton1Down:Connect(function()
	if dropdowntoggle == false then
		dropdowntoggle = true
		TS:Create(Icon_6, TweenInfo.new(0.2), {Rotation = 0}):Play()
	elseif dropdowntoggle == true then
		dropdowntoggle = false	
		TS:Create(Icon_6, TweenInfo.new(0.2), {Rotation = -90}):Play()
	end
end)

--end]]

